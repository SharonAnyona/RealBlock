{
    "canisters": {
        "func_types": {
            "type": "custom",
            "main": "canisters/func_types/index.ts",
            "candid": "canisters/func_types/index.did",
            "build": "npx azle func_types",
            "wasm": ".azle/func_types/func_types.wasm",
            "gzip": true,
            "declarations": {
                "output": "test/dfx_generated/func_types",
                "node_compatibility": true
            },
            "env": ["NOTIFIERS_PRINCIPAL"]
        },
        "notifiers": {
            "type": "custom",
            "main": "canisters/notifiers/index.ts",
            "candid": "canisters/notifiers/index.did",
            "build": "npx azle notifiers",
            "wasm": ".azle/notifiers/notifiers.wasm",
            "gzip": true,
            "declarations": {
                "output": "test/dfx_generated/notifiers",
                "node_compatibility": true
            },
            "env": ["NOTIFIERS_PRINCIPAL"]
        }
    }
}
