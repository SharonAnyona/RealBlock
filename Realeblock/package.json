{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy hello",
        "canister_deploy_mainnet": "dfx deploy --network ic hello",
        "canister_uninstall": "dfx canister uninstall-code hello",
        "canister_call_get_message": "dfx canister call hello getMessage",
        "canister_call_set_message": "dfx canister call hello setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "@dfinity/agent": "^0.15.6",
        "@dfinity/candid": "^0.15.6",
        "azle": "^0.17.1",
        "uuid": "^9.0.0"
    }
      
}

